{
    "name": "TimeSync",
    "version": "1.0",
    "description": "Sync and watch videos with others!",
    "content_scripts": [
        {
            "matches": ["http://www.crunchyroll.com/*"],
            "run_at": "document_idle",
            "js": ["socket.io.js", "synchronous.js"]
        }
    ],
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}